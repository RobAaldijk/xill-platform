<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<AnchorPane styleClass="root" stylesheets="@../styles.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<VBox styleClass="dialogWrapper" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<AnchorPane styleClass="dialogContent" VBox.vgrow="ALWAYS">
					<children>
						<VBox fx:id="vBox" alignment="CENTER_LEFT" styleClass="dialogFormWrapper" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
							<children>
								<TextFlow>
									<children>
										<Text fx:id="header" strokeType="OUTSIDE" strokeWidth="0.0" />
										<Text fx:id="seperation" strokeType="OUTSIDE" strokeWidth="0.0" />
										<Text fx:id="content" strokeType="OUTSIDE" strokeWidth="0.0" />
									</children>
								</TextFlow>
							</children>
						</VBox>
					</children>
				</AnchorPane>
				<HBox styleClass="buttonsWrapper" VBox.vgrow="NEVER">
					<children>
						<Button fx:id="yesBtn" defaultButton="true" mnemonicParsing="false" onAction="#yesBtnPressed" text="Yes" />
						<Button fx:id="noBtn" defaultButton="true" mnemonicParsing="false" onAction="#noBtnPressed" text="No" />
						<Button fx:id="applyBtn" defaultButton="true" mnemonicParsing="false" onAction="#applyBtnPressed" text="Apply" />
						<Button fx:id="okBtn" defaultButton="true" mnemonicParsing="false" onAction="#okayBtnPressed" text="Ok" />
						<Button fx:id="cancelBtn" cancelButton="true" mnemonicParsing="false" onAction="#cancelBtnPressed" text="Cancel" />
					</children>
				</HBox>
			</children>
		</VBox>
	</children>
</AnchorPane>