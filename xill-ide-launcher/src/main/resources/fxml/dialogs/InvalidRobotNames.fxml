<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="267.0" prefWidth="400.0" styleClass="root" stylesheets="@../styles.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<VBox styleClass="dialogWrapper" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<AnchorPane styleClass="dialogContent" VBox.vgrow="ALWAYS">
					<children>
						<VBox alignment="CENTER_LEFT" styleClass="dialogFormWrapper" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
							<children>
										<Text fx:id="txtTitle" strokeType="OUTSIDE" strokeWidth="0.0" text="There are 0 robots having invalid name.">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Text>
                        <TextArea fx:id="txtList" editable="false" prefHeight="200.0" prefWidth="200.0" />
                        <Text fx:id="txtTitle1" strokeType="OUTSIDE" strokeWidth="0.0" text="The names don't comply with the Xill robot naming convention." />
							</children>
						</VBox>
					</children>
				</AnchorPane>
				<HBox styleClass="buttonsWrapper" VBox.vgrow="NEVER">
					<children>
						<Button fx:id="skipBtn" mnemonicParsing="false" onAction="#skipBtnPressed" prefHeight="29.0" prefWidth="96.0" text="Skip invalid" />
						<Button fx:id="cancelBtn" cancelButton="true" defaultButton="true" mnemonicParsing="false" onAction="#cancelBtnPressed" text="Cancel" />
					</children>
				</HBox>
			</children>
		</VBox>
	</children>
</AnchorPane>
