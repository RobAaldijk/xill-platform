<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<HBox fx:id="hbxStatusBar" alignment="CENTER_LEFT" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<Label fx:id="lblStatusLbl" focusTraversable="false" text="Status:" />
				<Label fx:id="lblStatusVal" focusTraversable="false" text="Ready" />
				<AnchorPane HBox.hgrow="ALWAYS" />
				<Label fx:id="lblTimeRemaining" focusTraversable="false" text="" />
				<ProgressBar fx:id="barRobotProgress" focusTraversable="false" minWidth="200" prefWidth="400" progress="0.0" visible="false" />
			</children>
			<padding>
				<Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
			</padding>
		</HBox>
	</children>
</AnchorPane>
