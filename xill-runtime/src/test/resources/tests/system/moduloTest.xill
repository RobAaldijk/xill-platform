use System, Assert;

// For int
var result = 0 % 0;
Assert.equal(result, NaN);

// For long
var x = 1234567890123;

var y = x;
result = (x-y) % (y-x); 
Assert.equal(result, NaN);

// For BigInteger
x = 12345678901231234567890123;
y = x;
result = (x-y) % (y-x); 
Assert.equal(result, NaN);
