/*
* Specification: The addition assignment operator has the defined precedence
* Expected: 521, true
* Input: 5 :: 7 * 3, 5 :: 7 > 0
*/

use Assert;

var value = 1;
value ::= false || false;
Assert.equal(value, "1false"); // Precedence of ::= (1) is lower than || (2)