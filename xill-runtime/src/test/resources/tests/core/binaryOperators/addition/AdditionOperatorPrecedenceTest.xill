/*
* Specification: The addition operator has the defined precedence
* Expected: 14, true
* Input: 5 + 3 ^ 2, 5 + 5 < 5 + 6
*/

use Assert;

var value = 5 + 3 ^ 2;
Assert.equal(value, 14); // Precedence of + (6) is lower than ^ (7)

value = 5 + 5 < 5 + 6;
Assert.isTrue(value); // Precedence of + (6) is lower than < (5)
