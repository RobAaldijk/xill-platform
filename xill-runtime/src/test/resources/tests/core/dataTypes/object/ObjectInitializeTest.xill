/*
* Specification: Assigning a object to an variable MUST result in a variable that stores that object
* Input: {"1": 1, "2":"test","a":"test-a", "b": "test-b"}
* Expected: {"1": 1, "2":"test","a":"test-a", "b": "test-b"}
*/


use Assert;

var object = {"1": 1, "2":"test","a":"test-a", "b": "test-b"};

Assert.equal(object,{"1": 1, "2":"test","a":"test-a", "b": "test-b"});