/*
* Specification: Object.x MUST return the right value at that key
* Input: {"1": 1, "2":"test","a":"test-a", "b": "test-b"}
* Expected: 1 , 2 , "test-a", "test-b"
*/

use Assert;

var object = {"1": 1, "2":"test","a":"test-a", "b": "test-b"};

Assert.equal(object[1],1);
Assert.equal(object[1+1],"test");
Assert.equal(object["a"],"test-a");
Assert.equal(object.b,"test-b");